{
  "name": "@severstal/market-ui-library",
  "version": "0.8.1",
  "description": "",
  "main": ".dist/market-uilibrary.ssr.js",
  "module": ".dist/market-uilibrary.esm.js",
  "unpkg": ".dist/market-uilibrary.min.js",
  "types": ".dist/types/src/entry.esm.d.ts",
  "files": [
    ".dist/*",
    "src/lib-components/**/*.vue",
    "src/lib-components/**/*.scss",
    "src/lib-components/**/*.enum.ts",
    "src/theme",
    "src/assets/icons",
    "src/modules",
    "public"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "lint:js": "eslint . --ext .js,.vue,.ts  --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest --collect-coverage --forceExit --detectOpenHandles",
    "test:clearCache": "jest --clearCache",
    "test:husky": "jest -o --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "dev:storybook": "npm run test:generate-output && start-storybook -s ./public -p 6006",
    "build:storybook": "npm run test:generate-output && build-storybook -s ./public",
    "prebuild": "rimraf ./.dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./.dist/types/dev ./.dist/types/src/entry.d.ts",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "node-sass": "^6.0.1",
    "path-to-regexp": "^6.2.0",
    "sass-loader": "^10.2.0",
    "vee-validate": "^3.4.14",
    "vue-slider-component": "3.2.13",
    "vue2-datepicker": "3.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@jambonn/vue-concise-carousel": "^1.1.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-a11y": "6.3.8",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-jest": "6.3.8",
    "@storybook/addon-links": "6.3.8",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/vue": "6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/vue": "^5.8.2",
    "@types/jest": "^26.0.24",
    "@types/vue2-datepicker": "3.3.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "@vue/test-utils": "^1.2.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "2.23.0",
    "element-ui": "^2.15.6",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.15.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "minimist": "^1.2.5",
    "postcss": "^8.3.6",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svg-sprite": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.37.5",
    "storybook-addon-designs": "6.1.0",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.20.1",
    "stylelint-webpack-plugin": "^3.0.1",
    "svg-sprite-loader": "^6.0.9",
    "ts-jest": "^27.0.4",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^9.1.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14",
    "vue-slider-component": "3.2.13",
    "vue2-datepicker": "3.9.2"
  },
  "engines": {
    "node": ">=12"
  }
}
